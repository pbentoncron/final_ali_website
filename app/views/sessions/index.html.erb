<% title "Hair By Ali Cron - Fort Worth/DFW Hairstylist - Login " %>
<% meta_description "" %>
<% meta_keywords "" %>
<div class="container">
    <div class="col-sm-4 col-sm-offset-4">
        <h3>Login</h3>
        <form action="/sessions" method="POST">
            <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
            <div class="form-group">
              <input type="text" class="form-control" name="user[email]" placeholder="Email" />
            </div>
            <div class="form-group">
              <input type="password" class="form-control" name="user[password]" placeholder="Password" />
            </div>
            <div>
                <input class="btn btn-primary btn-lg sharp" type="submit" value="Login">
            </div>
        </form>
    </div>
</div>
